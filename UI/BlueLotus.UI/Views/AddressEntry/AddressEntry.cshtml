@{
    ViewBag.Title = ViewBag.MyString;
    Layout = "~/Views/Shared/_LayOutWithLatestKendoScript.cshtml";
}
<link rel="stylesheet" href="~/buttncss/style/cssNew.css" type="text/css" />
<style>
    #AdrGrid{
       height: 68vh!important;
    }
</style>
<div id="GridWindowAddressEntry">
    <div class="k-content clearfix" id="filterCont">
        <div class="col-md-12 control-icons" id="ButtonSet">
            <button class="control-button add" data-tooltip="Add" onclick="addAddress()"></button>
            <button class="control-button save" data-tooltip="Save" onclick="saveAddress()"></button>
            <button class="control-button load" data-tooltip="Load" onclick="loadAddress()"></button>
            <button class="control-button cancel" data-tooltip="Cancel" onclick="Cancel()"></button>
            <button class="control-button delete" data-tooltip="Delete" onclick="Delete()"></button>
        </div>

    </div>

    <br style="clear: both" />
    <div class="col-lg-12">
        <div class="form-group">
            <div class="row">
                <div id="HdrSec1_InptAdrCd">
                    <div class="col-md-2 col-sm-4">
                        <label id="HdrSec1_InptAdrCd_Lbl">Address Code:</label>
                    </div>
                    <div class="col-md-2 col-sm-4">
                        <input id="HdrSec1_InptAdrCd_Val" type="text" class="k-textbox" />
                    </div>
                </div>
                <div id="HdrSec1_InptAdrNm">
                    <div class="col-md-2 col-sm-4">
                        <label id="HdrSec1_InptAdrNm_Lbl">Address Name:</label>
                    </div>
                    <div class="col-md-2 col-sm-4">
                        <input id="HdrSec1_InptAdrNm_Val" type="text" class="k-textbox" />
                    </div>
                </div>
            </div>
        </div>

        <br style="clear: both" />

        <div id="AdrGrid"></div>
        <div>@Html.Partial("~/Views/HtnShared/PaginationControl.cshtml")</div>
    </div>
    <div>
        @Html.Partial("~/Views/AddressEntry/AddressTypeForm.cshtml")
    </div>
    <div>
        @Html.Partial("~/Views/AddressEntry/AddressContactInsert.cshtml")
    </div>
    <div>
        @Html.Partial("~/Views/AddressEntry/AddressEntryPopup.cshtml")
    </div>
    <div>
        @Html.Partial("~/Views/AddressEntry/AdrMas_ImageUploadPartial.cshtml")
    </div>   
</div>
<script id="command-template-Image" type="text/x-kendo-template">
    <a class="k-button k-grid-Imageselect">Image</a>
</script>
