        ItmCmpn_SelectWeb: '@Url.Content("~/PrcsDetCmpn/OrdDetCmpn_SelectWeb")'
    }

    var viewBag = {
        ObjKy: '@(ViewBag.ObjKy)',
        OurCd: '@(ViewBag.OurCd)',
        OurCd2: '@(ViewBag.OurCd2)',
        ObjCaptn: '@(ViewBag.ObjCaptn)'
    }



    $(document).ready(function () {
        document.getElementById("divSplitter").style.minHeight = "220px";
    });
</script>

