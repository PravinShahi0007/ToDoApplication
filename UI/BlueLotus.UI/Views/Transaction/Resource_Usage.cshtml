@{
    ViewBag.Title = "Resource_Usage";
    Layout = "~/Views/Shared/_LayOutWithLatestKendoScript.cshtml";
}
<link rel="stylesheet" href="~/buttncss/style/cssNew.css" type="text/css" />
<style>
    .invoiceGrid .k-grid-content > table > tbody > tr {
        height: 2px;
        font-size: 11px;
    }
</style>

<div id="idMainDivDetailInvoice">

    <div class="col-md-12 control-icons" id="ButtonSet">
        <div id="ButtonSec1" class="col-md-9 col-sm-9">
            <button id="ButtonSec1_SaveAndPrintAndNew" class="control-button ok" onclick="SaveAndPrintAndNew()" data-tooltip="SAVE &amp; PRINT &amp; New"></button>
            <button class="control-button save" id="btnSave" onclick="Save()" title="Save"></button>
            <button class="control-button save_and_new" onclick="SaveUpdate('saveandnew')" data-tooltip="Save And New"> </button>
            <button class="control-button add" onclick="Add('Add')" data-tooltip="Add"> </button>
            <button class="control-button cancel" id="btnCancel" onclick="ComClearFunction()" title="Cancel"></button>
        </div>
    </div>


    <div id="HdrPart">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="wr_formGroup" id="HdrSec1">
                <div class="row form-group">
                    <!-- Trn No -->
                    <div class="col-lg-3 col-md-3 col-sm-6" id="HdrSec1_InptTrnNo">
                        <div class="row">
                            <div class="col-lg4 col-md-4 col-sm-12">
                                <label id="HdrSec1_InptTrnNo_Lbl">Trn No:</label>
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-12">
                                <input id="ResourceTrnNo" readonly="readonly" type="text" class="k-textbox width-100" />
                            </div>
                        </div>
                    </div>
                    <!-- /Trn No -->
                    <!-- Trn Date -->
                    <div class="col-lg-3 col-md-3 col-sm-6" id="HdrSec1_DatPicDate">
                        <div class="row">
                            <div class="col-lg-4 col-md-4 col-sm-12 p-r-0-lg"><label id="HdrSec1_DatPicDate_Lbl">Trn Date:</label></div>
                            <div class="col-lg-7 col-md-7 col-sm-12">
                                <input data-cpypst-blsmartcpypaste="Date" id="HdrSec1_DatPicDate_Val" class="width-100" data-role="datepicker" data-bind="value: Date" />
                            </div>
                        </div>
                    </div>
                    <!-- /Trn Date -->
                    <!-- Project -->
                    <div class="col-lg-6 col-md-6 col-sm-12" id="HdrSec1_CmbProject">
                        <div class="row">
                            <div class="col-lg-2 col-md-2 col-sm-12">
                                <label id="HdrSec1_CmbProject_Lbl">Project:</label>
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-12 p-r-0-lg">
                                <div data-cpypst-blsmartcpypaste="PrjId" id="HdrSec1_CmbProject_Cd" class="width-100"></div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-12 p-l-2-lg">
                                <div data-cpypst-blsmartcpypaste="PrjNm" id="HdrSec1_CmbProject_Nm" class="width-100"></div>
                            </div>
                        </div>
                    </div>
                    <!-- /Project -->                  
                </div>             
            </div>
        </div>      
    </div>

    <div id="GridWin" style="clear:both;">
        <div class="col-md-12 wr_formGroup" id="ChildHdrpart" >
            <div id="HdrSec2" style="margin-left: -26px">
                <div class="form-group" id="HdrSec2_CmbTask">

                    <div class="col-md-2 col-sm-2">
                        <label id="HdrSec2_CmbTask_Lbl">Task:</label>
                    </div>
                    <div class="col-md-10 col-sm-10">
                        <div data-cpypst-blsmartcpypaste="TaskId" id="HdrSec2_CmbTask_Cd" class="m-b-2"></div>
                        <div data-cpypst-blsmartcpypaste="TaskNm" id="HdrSec2_CmbTask_Nm" style="width:100%"></div>
                    </div>

                </div>
                <div class="form-group" id="HdrSec2_CmbResource">

                    <div class="col-md-2 col-sm-2">
                        <label id="HdrSec2_CmbResource_Lbl">Resource:</label>
                    </div>
                    <div class="col-md-10 col-sm-10">
                        <div data-cpypst-blsmartcpypaste="ResCd" id="HdrSec2_CmbResource_Cd" class="m-b-2"></div>
                        <div data-cpypst-blsmartcpypaste="ResNm" id="HdrSec2_CmbResource_Nm" style="width:100%"></div>
                    </div>

                </div>

                <div class="form-group" id="HdrSec2_CmbUnit">

                    <div class="col-md-5 col-sm-4">
                        <label id="HdrSec2_CmbUnit_Lbl">Unit:</label>
                    </div>
                    <div class="col-md-4 col-sm-4">
                        <div data-cpypst-blsmartcpypaste="ResCd" id="HdrSec2_CmbUnit_Cd"></div>
                    </div>

                </div>

                <div class="form-group" id="HdrSec2_NmricQty">

                    <div class="col-md-5 col-sm-5">
                        <label id="HdrSec2_CmbResource_Lbl">Resource Qty:</label>
                    </div>
                    <div class="col-md-4 col-sm-4" style="margin-left:-14px">
                        <input data-cpypst-blsmartcpypaste="ResQty" id="HdrSec2_NmricQty_Val" />
                    </div>

                </div>

                <div class="form-group" id="HdrSec2_NmricRate">
                    <div class="col-md-3 col-sm-3">
                        <label id="HdrSec2_CmbUnit_Lbl">Rate:</label>
                    </div>
                    <div class="col-md-4 col-sm-3">
                        <input id="HdrSec2_NmricRate_Val" />
                    </div>
                </div>

                <div class="form-group" id="HdrSec2_NmricLiNo" style="display:none;">
                    <input data-cpypst-blsmartcpypaste="ResQty" id="HdrSec2_NmricLiNo_Val" />

                </div>
            </div>
        </div>
    </div>
</div>

