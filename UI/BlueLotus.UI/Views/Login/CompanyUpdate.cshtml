@{
    ViewBag.Title = "Company Update";
    Layout = "~/Views/Shared/_LayOutWithLatestKendoScript.cshtml";
    <link rel="stylesheet" href="~/buttncss/style/cssNew.css" type="text/css" />
}
<link rel="stylesheet" href="~/Images/BLBanner/style.css" type="text/css" />
<link rel="stylesheet" href="~/buttncss/style/ResponsiveMax.css" type="text/css" />

<div id="UpdateForm" class="col-lg-12">
    <div class="k-content clearfix" id="filterCont">
        <button class="control-button save" onclick="UpdateCompany()" title="Save">Save</button>
        <button class="control-button cancel" onclick="" title="Clear"> cancel</button>
    </div>
    @*<div style="height:30px; background-color:cornflowerblue; margin-top:-8px;"><h4 style="margin-left:30px; color:white;">Update Company</h4></div>*@
    <hr style="margin-top:-1px;" />

    <div class="form-group">
        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Company Code</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="CCd" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Company Name</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="CName" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Address</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Addrs" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Town</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Town" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>City</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="City" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Country</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Cntry" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Tel No 1</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Tel1" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Tel No 2</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Tel2" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Tel No 3</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Tel3" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Fax</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Fax" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Email</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Email" type="email" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Tax No</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="taxno" type="text" class="k-textbox" /></div>             
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Web Site</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Web" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>Moto</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Moto" type="text" class="k-textbox" /></div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Financial Year st Date</label></div>
            <div class="col-md-3 col-sm-3">
                <input id="FYSD"                       
                       data-role="datepicker"
                       data-bind="value: FYSD" />
            </div>
            <div class="col-md-2 col-sm-2"><label>Tran Confirm Date</label></div>
            <div class="col-md-3 col-sm-3">
                <input id="TCD"
                       data-role="datepicker"
                       data-bind="value: TCD" />
            </div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2"><label>Remark</label></div>
            <div class="col-md-3 col-sm-3"><input style="width:100%;" id="Remark" type="text" class="k-textbox" /></div>
            <div class="col-md-2 col-sm-2"><label>EPF Reg.No</label></div>
            <div class="col-md-4 col-sm-4"><input id="EPFReg" type="text" class="k-textbox" /></div>
        </div>

        @*<div class="row">
            <div class="col-md-2 col-sm-2"><label>Remark</label></div>
            <div id="HdrSec2_CmbLoc_Cd"></div>
            
        </div>*@
        

    </div>

    <hr>
    <div style="margin-top:-10px;display:none" class="form-group">

        <div class="row">
            <div class="col-md-2 col-sm-2">
                <label>is Multi Project</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="MultiProject" name="Multi Project" type="checkbox" />
            </div>

            <div class="col-md-2 col-sm-2">
                <label>is Alert</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="Alert" name="Alert" type="checkbox" />
            </div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2">
                <label>is Multi BU</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="MultiBU" name="Multi BU" type="checkbox" />
            </div>

            <div class="col-md-2 col-sm-2">
                <label>is Monthly Depreciation</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="MonthlyDepreciation" name="Monthly Depreciation" type="checkbox" />
            </div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2">
                <label>is Multi Location</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="MultiLocation" name="Multi Location" type="checkbox" />
            </div>

            <div class="col-md-2 col-sm-2">
                <label>Part No</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="PartNo" name="Part No" type="checkbox" />
            </div>
        </div>

        <div class="row">
            <div class="col-md-2 col-sm-2">
                <label>is Multi Currency</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="MultiCurrency" name="Multi Currency" type="checkbox" />
            </div>

            <div class="col-md-2 col-sm-2">
                <label>is active</label>
            </div>
            <div class="col-md-1 col-sm-1 ">
                <input id="isactive" name="Deactive" type="checkbox" />
            </div>
        </div>

    </div>

</div>
<script>

    var viewBag = {
        ObjKy: '@(ViewBag.ObjKy)',
        OurCd: '@(ViewBag.OurCd)',
        OurCd2: '@(ViewBag.OurCd2)',
        ObjCaptn: '@(ViewBag.ObjCaptn)'
    }

    var URLGetCompanyDetails = '@Url.Content("~/Login/CmpDetEnt_Select")';
    var URLUpdateCompanyDetails = '@Url.Content("~/Login/CmpnyUpdate")';
</script>
<script src="~/Scripts/HtnScripts/Login/CompanyUpdate.js"></script>
