<script>

    $(document).ready(function () {
        $('#PrcsSchDetCmpnPartial_SS_Div').hide();
    });

    var todayDate = new Date();
    var dd = todayDate.getDate();
    var mm = todayDate.getMonth() + 1; //January is 0!

    var yyyy = todayDate.getFullYear();
    if (dd < 10) {
        dd = '0' + dd
    }
    if (mm < 10) {
        mm = '0' + mm
    }
    var todayDate = dd + '/' + mm + '/' + yyyy;

    var globalVariable = {
        PrcsDetKy: 1,
        FinItmKy: 1,
        TodayDate: todayDate
    }

    var urlObjMas = {
        UsrObjPrp_SelectWeb: '@Url.Action("UsrObjPrp_SelectWeb", "ObjMas")',
        UsrObjPrp_SelectMulti: '@Url.Action("UsrObjPrp_SelectMulti", "ObjMas")'
    }

    var urlCmpnBld = {
        @*SaveItmCmpn: '@Url.Content("~/OrdDetCmpn/SaveOrdDetCmpn")',
        ItmCmpn_SelectWeb: '@Url.Content("~/OrdDetCmpn/OrdDetCmpn_SelectWeb")'*@
