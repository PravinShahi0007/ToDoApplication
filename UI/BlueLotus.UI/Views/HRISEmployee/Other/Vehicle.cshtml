@{
    ViewBag.Title = "Vehicle";
    Layout = "~/Views/Shared/_LayOutWithLatestKendoScript.cshtml";

}
<link rel="stylesheet" href="~/buttncss/style/cssNew.css" type="text/css" />

<div>
    <div class="k-content clearfix" id="filterCont">

        <div class="col-md-12 control-icons" id="ButtonSet">

            <button class="control-button save" onclick="SaveUpdate('save') " data-tooltip="Save"> </button>
            <button class="control-button save_and_new" onclick="SaveUpdate('saveandnew')" data-tooltip="Save And New"> </button>
            <button class="control-button update" onclick="SaveUpdate('Update')" data-tooltip="Update"> </button>
            <button class="control-button cancel" onclick="clearfunction()" data-tooltip="Cancel"> </button>

        </div>
        <div>
            @Html.Partial("~/Views/HRISEmployee/EmpGetDet.cshtml")
        </div>
        <div>
            @Html.Partial("~/Views/HRISEmployee/GetEmp.cshtml")
        </div>
        <div id="HdrPart">
            <div class="col-md-3 col-sm-6">
                <div class="wr_formGroup">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label>From Date:</label>
                            </div>
                            <div class="col-md-6 col-sm-8 ">
                                <input placeholder="From Date" id="fromDate" type="text" class="k-textbox" />

                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label>To Date:</label>
                            </div>
                            <div class="col-md-6 col-sm-8 ">
                                <input placeholder="To Date" id="toDate" type="text" class="k-textbox" />

                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label>Vehicle Type:</label>
                            </div>
                            <div class="col-md-6 col-sm-8 ">
                                <div id="cmbVehicleType" data-cpypst-blsmartcpypaste="cmbVehicleType"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3 col-sm-6">
                <div class="wr_formGroup">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label>Model:</label>
                            </div>
                            <div class="col-md-6 col-sm-8 ">
                                <input placeholder="Model" id="model" type="text" class="k-textbox" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <label>Vehicle Reg No:</label>
                            </div>
                            <div class="col-md-6 col-sm-8 ">
                                <input placeholder="Vehicle Reg No" id="vehicleRegNo" type="text" class="k-textbox" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <br style="clear:both" />
    <div class="col-md-12 col-sm-12">
        <div id="VehicleDetGrid">

        </div>

    </div>
</div>
<div>
    @* @Html.Partial("~/Views/Journal/JournalFind.cshtml")*@
</div>

<script>
    var OurCode = "@(ViewBag.OurCd)";
    var urlHRISEmployeeGetCdMas_LookupWeb = '@Url.Content("~/HRISEmployee/GetCdMas_LookupWeb")';

    var urlInsertVehicleDet = '@Url.Content("~/HRISEmployee/HRISInsertVehicleDet")';
    var urlGetVehicleDet = '@Url.Content("~/HRISEmployee/HRISGetVehicleDet")';
</script>

<style>
    .ob-Center {
        text-align: center;
    }

    .ob-right {
        text-align: center;
    }
</style>

<script src="~/Scripts/HtnScripts/HRISEmployee/Vehicle.js"></script>

