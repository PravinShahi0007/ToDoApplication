@{
    ViewBag.Title = "PopUpCredit";
}
<input type="hidden" name="BUKyCredit" id="BUKyCredit" readonly="readonly" />
<input type="hidden" name="PRJKyCredit" id="PRJKyCredit" readonly="readonly" />
<div id="PopUpForCredit" style="display: none">
    <div>
        <div class="col-md-12 control-icons" id="ButtonSet">

            <button class="control-button save" onclick="saveCredit()" data-tooltip="save"> </button>
            <button class="control-button back" onclick="closeCredit()" data-tooltip="back"> </button>
            <button class="control-button add" onclick="insertCreditRw()"data-tooltip="add"> </button>
        </div>
        <br style="clear: both"/>
        <div class="col-md-12 col-sm-12">
            <div id="HdrPart">
                <div class="col-md-6 col-sm-6">
                    <div class="wr_formGroup">
                        <div class="form-group">
                            <div class="row">

                                <input type="hidden" name="AccKyCredit" id="AccKyCredit" readonly="readonly"/>
                                <div class="col-md-5 col-sm-6">
                                    <label>Account Code:</label>
                                </div>
                                <div class="col-md-5 col-sm-8 ">
                                    <input type="text" name="AccCdCredit" id="AccCdCredit" readonly="readonly" class="k-textbox"/>

                                </div>

                            </div>
                            <div class="row">

                                <div class="col-md-5 col-sm-6">
                                    <label> Account Name:</label>
                                </div>
                                <div class="col-md-5 col-sm-8 ">
                                    <input type="text" name="AccNmCredit" id="AccNmCredit" readonly="readonly" class="k-textbox"/>

                                </div>

                            </div>

                        </div>
                    </div>
                </div>

            </div>


        </div>
        <br style="clear: both"/>
        <div class="col-md-12 col-sm-12">
            <div id="PopUpCredit"></div>

        </div>

    </div>
</div>
<script src="~/Scripts/HtnScripts/AllAccounts/PopUpCredit.js"></script>
<script>

    var UrlCreditList = '@Url.Content("~/ManageAllAccount/CreditsPop")';
    var UrlCreditListUpdate = '@Url.Content("~/ManageAllAccount/CreditUpdate")';
</script>